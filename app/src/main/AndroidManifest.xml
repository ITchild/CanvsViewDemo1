<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fei.canvsviewdemo">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".CanvnsApplication"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.LineChartActivity"
            android:screenOrientation="landscape"></activity>

        <activity
            android:name=".activity.ColumnChartActivity"
            android:screenOrientation="landscape"></activity>

        <activity
            android:name=".activity.WebServiceActivity"
            android:screenOrientation="landscape"></activity>

        <activity android:name=".activity.DialogActivity"
            android:screenOrientation="landscape"
            android:theme="@style/ActivityDialogTheme"></activity>

        <activity android:name=".activity.buletooth.buleToothActivity"
            android:alwaysRetainTaskState="true"></activity>

    </application>

</manifest>